{
  "extends": ["@sliit-foss/eslint-config-internal"],
  "parser": "babel-eslint", // This line is required to fix "unexpected token" errors
  "env": {
    "browser": false,
    "node": true,
    "mocha": true,
    "es6": true
  },
  "rules": {
    "no-sparse-arrays": "off",
    "no-nested-ternary": "off",
    "newline-per-chained-call": "off"
  }
}
